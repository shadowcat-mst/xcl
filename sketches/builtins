set = set;
# let . = dot;
[Scope.current.val let] = \Scope.current().val;
let var = \Scope.current().var;
let fexpr = Fexpr.make;
let => = let lambda = Lambda.make;
let % = let dict = Dict.make;
let current_scope = Scope.current;
let string = String.make;
let == = .eq;
let != = .ne;
let < = .lt;
let > = .gt;
let <= = .le;
let >= = .ge;
let + = .plus;
let - = .minus;
let * = .multiply;
let / = .divide;
let ++ = .concat;
let \ = escape;
let $ = id;
let ? = result_of;
let ! = let not = Bool.not;
