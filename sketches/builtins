set = set;
[Scope.val val] = Scope.val;
val var = Scope.var;
val fexpr = Fexpr.make;
val => = val lambda = Lambda.make;
val % = val dict = Dict.make;
val current_scope = Scope.current;
val string = String.make;
val == = .eq;
val != = .ne;
val < = .lt;
val > = .gt;
val <= = .le;
val >= = .ge;
val + = .plus;
val - = .minus;
val * = .multiply;
val / = .divide;
val ++ = .concat;
val \ = escape;
