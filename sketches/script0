use strict;
use warnings;
use feature 'say';
use XCL::Values;
use JSON::MaybeXS ();

sub encode_json { Bytes(JSON::MaybeXS::encode_json($_[0])) }

sub say { CORE::say @_ }

use xcl::script;

say 'Hello world';

let : = fexpr (s, n) {
  let k = n.name_to_string();
  v => { $ (k, v) }
}

say [ encode_json %(:x 1, :y 2) ];
